    .leadership-group  .shows-player {width: 620px}

.leadership-group .playa-playa {float: none}
.leadership-group .listing-box {margin-left: 0; width: 620px; background: none}
.leadership-group .playa-list {height: auto; overflow: visible; margin-bottom: 10px; border-bottom: 3px double #CDCDCD; padding-bottom: 10px}
.full-wrap .playa-items {float: left; width: 180px;margin-right: 20px}
.full-wrap .playa-items:first-child {margin-left: 20px}
.full-wrap .playa-items:last-child {margin-right: 0}
.leadership-group .playa-items h2 {font-size: 14px; line-height: 1.2; text-align: center}
.leadership-group .playa-items h2 a {line-height: 1.2}
.leadership-group .playa-items p {font-size: 0.75em; margin-left: 10px}

